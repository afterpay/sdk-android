<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Afterpay">
        <attr name="afterpayStyle" format="enum">
            <!-- "default" is not a valid resource name (reserved Java keyword) so appending "_style" -->
            <enum name="default_style" value="0" />
            <enum name="alt" value="1" />
            <enum name="monochromeDark" value="2" />
            <enum name="monochromeLight" value="3" />
        </attr>
        <attr name="afterpayButtonText" format="enum">
            <enum name="payNow" value="0" />
            <enum name="buyNow" value="1" />
            <enum name="checkout" value="2" />
            <enum name="placeOrder" value="3" />
        </attr>
    </declare-styleable>
</resources>
